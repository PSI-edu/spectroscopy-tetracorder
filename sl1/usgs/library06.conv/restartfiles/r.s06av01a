SPECPR_Restart=2.00      # Restart Version
#
# file names
#
ivfl=s06av01a                                                                        
iwfl=/dev/null                                                                       
idfl=/dev/null                                                                       
iufl=/dev/null                                                                       
iyfl=splib06b                                                                        
isfl=/dev/null                                                                       
ilfl=spoolfile                                                                       
irfl=restartfiles/r.s06av01a                                                         
#
# protection number for open files (v,w,d,u,y,s)
#
iprtv=          8219  # device protection v
iprtw=             0  # device protection w
iprtd=             0  # device protection d
iprtu=             0  # device protection u
iprty=        -13744  # device protection y
iprts=             0  # device protection s
#
# short 8 character names associated with file device letters
#
isavt=      s06av01a  # file device letter v
iwdgt=      *unasnd*  # file device letter w
iwrkt=      *unasnd*  # file device letter d
inmu=       *unasnd*  # file device letter u
inmy=       splib06b  # file device letter y
#
# plot control values (real number format: ex. 0.23E+01)
#
wmina=  0.300000E+00  # plot min wavelength
wmaxa=  0.255000E+01  # plot max wavelength
bbnd=   0.000000E+00  # plot min reflectance
